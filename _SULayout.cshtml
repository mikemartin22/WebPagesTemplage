<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Salisbury University - @Page.Title</title>

    <link rel="stylesheet" href="Styles/app_style.css" media="screen" />
    <link rel="stylesheet" href="Styles/app_style_RD.css" media="screen" />
    <link rel="stylesheet" href="Styles/app_print.css" media="print" />
    <link rel="stylesheet" href="Styles/css/jquery-ui/jquery-ui-1.10.3.custom.css" media="screen" />
    <link rel="stylesheet" href="Styles/css/jquery-ui/jquery-ui-plugins.css" media="screen" />
    <link rel="stylesheet" href="Styles/bootstrap/3.1.1/css/SU.css">
    <link rel="stylesheet" href="Styles/bootstrap/3.1.1/css/bootstrap.css">
    <link rel="stylesheet" href="Styles/StyleSheet.css">
    @*<link type="text/css" href="@Href("~/Styles/StyleSheet.css")" rel="stylesheet" />*@

    @*<script src="https://code.jquery.com/jquery-1.12.4.min.js" type="text/javascript"></script>*@
    <script src="Styles/jquery/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="https://code.jquery.com/jquery-migrate-1.4.1.js"></script>
    <script src="Styles/jquery/jquery.validate.js" type="text/javascript"></script>
    <script src="Styles/jquery/jquery-ui-1.10.3.min.js" type="text/javascript"></script>
    <script src="Styles/jquery/jquery.ui.datePicker.js" type="text/javascript"></script>
    <script src="Styles/jquery/jquery.ui.timepicker.js" type="text/javascript"></script>
    <script src="Styles/js/moment.min.js" type="text/javascript"></script>
    <script>
        // This gets the appRoot from the server and puts it
        // in js to create a variable I can access from client side.
        // It's an odd workaraound, but it works.
        @{
         var outScript = "var appRoot = " + "\"" + @Href("~") + "/\"";
         }
        @Html.Raw(outScript);

        //this deals with clients with no js...
        document.write('<style>.noscript { display:none }</style>');
    </script>
</head>
<body>
    <div id="header">
        <header>
            <table style="width: 100%">
                <tr>
                    <td>
                        <span style="font-size: xx-large; color: white; font-weight: bold;">@Page.Title</span><br />
                        <span style="font-size: x-large; color: white; font-weight: bold;">@Page.SubTitle</span>
                    </td>
                    <td>
                        <a href="http://www.salisbury.edu/"><img alt="SU Logo" src="@Href("Styles/images/su_logo_white.gif")" style="border-width: 0px; float: right; margin-right: 10px;" width="200" height="63" /></a>
                    </td>
                </tr>
            </table>
        </header>
    </div>
    <div id="main" class="container">
        <!--Menu area...-->
      <div style="text-align: right">            
            <a class="input-button" href="@Href("/logout/")">Log Out</a>
        </div>
        
        <hr>
        <!--Body area-->
        @RenderBody()

    </div>
    <div id="footer">
        <p>Copyright &copy Salisbury University @DateTime.Now.Year </p>
        <a href="http://www.salisbury.edu/weboffice/">Web Development Office</a>&nbsp;&nbsp;&bull;&nbsp;&nbsp;<a href="mailto:webmaster@salisbury.edu">webmaster@salisbury.edu</a>
    </div>
</body>
</html>